{
  "manifest_version": 2,
  "name": "YouBlock",
  "version": "0.1",

  "description": "YouTube content blocker that you can manage!",

  "icons": {
    "256": "img/youblock_icon.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["youblock.js"]
    }
  ]
}